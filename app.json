  "description": "Telegram userbot dibuat dengan nodejs menggunakan duagam",
  "keywords": [
    "telegram",
    "userbot"
  ],
  "repository": "https://github.com/bnekozu/nekozuuserbot",
  "env": {
    "api_id": {
      "description": "Dapatkan dari my.telegram.org.",
